{
  "make": "aerona",
  "modbus": {
    "models": ["Chofu/Grant Aerona3"],
    "connection": {
      "baudrate": 19200,
      "bytesize": 8,
      "parity": "N",
      "stopbits": 2,
      "method": "rtu",
      "timeout": 5,
      "delay": 0,
      "message_wait_milliseconds": 30
    },
    "registers": {
      "input_registers": [
        {
          "name": "Return_Water_Temperature",
          "address": 0,
          "register_type": "input",
          "unit": "°C",
          "scale": 1,
          "description": "Return water temperature",
          "common_interface": "water_inlet_temp"
        },
        {
          "name": "Discharge_Temperature",
          "address": 2,
          "register_type": "input",
          "unit": "°C",
          "scale": 1,
          "description": "Discharge temperature",
          "common_interface": "discharge_temp"
        },
        {
          "name": "Defrost_Temperature",
          "address": 5,
          "register_type": "input",
          "unit": "°C",
          "scale": 1,
          "description": "Defrost temperature"
        },
        {
          "name": "Outdoor_Air_Temperature",
          "address": 6,
          "register_type": "input",
          "unit": "°C",
          "scale": 1,
          "description": "Outdoor air temperature",
          "common_interface": "outdoor_temperature"
        },
        {
          "name": "Suction_Temperature",
          "address": 8,
          "register_type": "input",
          "unit": "°C",
          "scale": 1,
          "description": "Suction temperature",
          "common_interface": "suction_temp"
        },
        {
          "name": "Outgoing_Water_Temperature",
          "address": 9,
          "register_type": "input",
          "unit": "°C",
          "scale": 1,
          "description": "Outgoing water temperature",
          "common_interface": "water_outlet_temp"
        },
        {
          "name": "Operating_Mode",
          "address": 10,
          "register_type": "input",
          "description": "Operating mode: 0=Off, 1=Heating, 2=Cooling",
          "common_interface": "operating_mode"
        },
        {
          "name": "Zone1_Room_Temperature",
          "address": 11,
          "register_type": "input",
          "unit": "°C",
          "scale": 0.1,
          "description": "Zone 1 room temperature",
          "common_interface": "zone1_room_temp"
        },
        {
          "name": "Zone2_Room_Temperature",
          "address": 12,
          "register_type": "input",
          "unit": "°C",
          "scale": 0.1,
          "description": "Zone 2 room temperature",
          "common_interface": "zone2_room_temp"
        },
        {
          "name": "DHW_Operating_Mode",
          "address": 13,
          "register_type": "input",
          "description": "DHW mode: 0=Disable, 1=Comfort, 2=Economy, 3=Force"
        },
        {
          "name": "DHW_Tank_Temperature",
          "address": 16,
          "register_type": "input",
          "unit": "°C",
          "scale": 1,
          "description": "DHW tank temperature",
          "common_interface": "dhw_tank_temp"
        },
        {
          "name": "Humidity_Sensor",
          "address": 20,
          "register_type": "input",
          "unit": "%",
          "description": "Humidity sensor reading"
        },
        {
          "name": "Current_Error_Code",
          "address": 21,
          "register_type": "input",
          "description": "Current error code",
          "common_interface": "error_code"
        },
        {
          "name": "Previous_Error_Code_1",
          "address": 22,
          "register_type": "input",
          "description": "Previous error code 1"
        },
        {
          "name": "Previous_Error_Code_2",
          "address": 23,
          "register_type": "input",
          "description": "Previous error code 2"
        },
        {
          "name": "Previous_Error_Code_3",
          "address": 24,
          "register_type": "input",
          "description": "Previous error code 3"
        },
        {
          "name": "Previous_Error_Code_4",
          "address": 25,
          "register_type": "input",
          "description": "Previous error code 4"
        },
        {
          "name": "Previous_Error_Code_5",
          "address": 26,
          "register_type": "input",
          "description": "Previous error code 5"
        },
        {
          "name": "Plate_Heat_Exchanger_Temperature",
          "address": 32,
          "register_type": "input",
          "unit": "°C",
          "scale": 1,
          "description": "Plate heat exchanger temperature"
