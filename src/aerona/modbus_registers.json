      "holding_registers": [
        {
          "name": "Heating_Zone1_Fixed_Water_Setpoint",
          "address": 2,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 25,
          "max": 55,
          "description": "Fixed outgoing water set point for heating zone 1"
        },
        {
          "name": "Heating_Zone1_Max_Water_Temperature",
          "address": 3,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 25,
          "max": 55,
