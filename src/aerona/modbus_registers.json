        {
          "name": "Heating_Zone1_Min_Water_Temperature",
          "address": 4,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 25,
          "max": 55,
          "description": "Minimum outgoing water temperature for heating zone 1"
        },
        {
          "name": "Heating_Zone1_Max_Outdoor_Temperature",
          "address": 5,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "description": "Maximum outdoor air temperature correlation for heating zone 1"
        },
        {
          "name": "Heating_Zone1_Min_Outdoor_Temperature",
          "address": 6,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "description": "Minimum outdoor air temperature correlation for heating zone 1"
        },
        {
          "name": "Heating_Zone2_Fixed_Water_Setpoint",
          "address": 7,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 25,
          "max": 55,
          "description": "Fixed outgoing water set point for heating zone 2"
        },
        {
          "name": "Heating_Zone2_Max_Water_Temperature",
          "address": 8,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 25,
          "max": 55,
          "description": "Maximum outgoing water temperature for heating zone 2"
        },
        {
          "name": "Heating_Zone2_Min_Water_Temperature",
          "address": 9,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 25,
          "max": 55,
          "description": "Minimum outgoing water temperature for heating zone 2"
        },
        {
          "name": "Heating_Zone2_Max_Outdoor_Temperature",
          "address": 10,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "description": "Maximum outdoor air temperature correlation for heating zone 2"
        },
        {
          "name": "Heating_Zone2_Min_Outdoor_Temperature",
          "address": 11,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "description": "Minimum outdoor air temperature correlation for heating zone 2"
        },
        {
          "name": "Cooling_Zone1_Fixed_Water_Setpoint",
          "address": 13,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 5,
          "max": 25,
          "description": "Fixed outgoing water set point for cooling zone 1"
        },
        {
          "name": "Cooling_Zone2_Fixed_Water_Setpoint",
          "address": 18,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 5,
          "max": 25,
          "description": "Fixed outgoing water set point for cooling zone 2"
        },
        {
          "name": "DHW_Priority",
          "address": 26,
          "register_type": "holding",
          "description": "DHW production priority: 0=Standard, 1=Priority"
