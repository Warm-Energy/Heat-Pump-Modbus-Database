        {
          "name": "DHW_Comfort_Temperature",
          "address": 27,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 40,
          "max": 60,
          "description": "DHW comfort temperature"
        },
        {
          "name": "DHW_Economy_Temperature",
          "address": 28,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 40,
          "max": 60,
          "description": "DHW economy temperature"
        },
        {
          "name": "DHW_Force_Temperature",
          "address": 29,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "min": 50,
          "max": 70,
          "description": "DHW force temperature"
        },
        {
          "name": "Pump_Run_Time",
          "address": 37,
          "register_type": "holding",
          "unit": "min",
          "description": "Pump run time after compressor off"
        },
        {
          "name": "Pump_Frequency",
          "address": 38,
          "register_type": "holding",
          "unit": "Hz",
          "description": "Pump frequency setting"
        },
        {
          "name": "Compressor_Start_Delay",
          "address": 39,
          "register_type": "holding",
          "unit": "min",
          "description": "Compressor start delay"
        },
        {
          "name": "Frost_Protection_Start_Temperature",
          "address": 50,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "description": "Temperature at which frost protection starts"
        },
        {
          "name": "Frost_Protection_Stop_Temperature",
          "address": 51,
          "register_type": "holding",
          "unit": "°C",
          "scale": 0.5,
          "description": "Temperature at which frost protection stops"
        }
      ],
      "coils": [
        {
          "name": "Erase_Error_History",
          "address": 0,
          "coil_type": "coil",
          "description": "Erase error history"
        },
        {
          "name": "Reboot_Operation",
          "address": 1,
          "coil_type": "coil",
          "description": "Operation at reboot after blackout: 0=disable, 1=enable"
        },
        {
          "name": "Heating_Zone1_Water_Setpoint_Mode",
          "address": 2,
          "coil_type": "coil",
          "description": "0=Fixed set point, 1=Climatic curve"
        },
        {
          "name": "Anti_Legionella_Function",
          "address": 6,
          "coil_type": "coil",
          "description": "Anti-legionella function: 0=disable, 1=enable"
        },
        {
          "name": "HP_Unit_Control_Mode",
          "address": 7,
          "coil_type": "coil",
          "description": "HP unit turn on/off based on: 0=Room set point, 1=Water set point"
        }
      ],
      "discrete_inputs": [
        {
          "name": "Configurable_Input_Heating_Enable",
          "address": 3,
          "discrete_type": "discrete",
          "description": "Configurable input (terminal 20-21) / heating enable"
        }
      ],
      "sensors": [
        {
          "name": "Return_Water_Temperature",
          "address": 0,
          "register_type": "input",
          "unit": "°C",
          "scale": 1,
          "scan_interval": 30,
          "precision": 1
