        {
          "name": "Zone2_Room_Temperature",
          "address": 12,
          "register_type": "input",
          "unit": "°C",
          "scale": 0.1,
          "scan_interval": 60,
          "precision": 1
        },
        {
          "name": "DHW_Tank_Temperature",
          "address": 16,
          "register_type": "input",
          "unit": "°C",
          "scale": 1,
          "scan_interval": 60,
          "precision": 1
        },
        {
          "name": "Operating_Mode",
          "address": 10,
          "register_type": "input",
          "scan_interval": 30
        },
        {
          "name": "DHW_Operating_Mode",
          "address": 13,
          "register_type": "input",
          "scan_interval": 60
        },
        {
          "name": "Current_Error_Code",
          "address": 21,
          "register_type": "input",
          "scan_interval": 60
        }
      ],
      "switches": [
        {
          "name": "Reboot_Operation",
          "address": 1,
          "write_type": "coil",
          "command_on": 1,
          "command_off": 0
        },
        {
          "name": "Anti_Legionella_Function",
          "address": 6,
          "write_type": "coil",
          "command_on": 1,
          "command_off": 0
        }
      ],
      "climates": [
        {
          "name": "Heating_Zone_1",
          "address": 11,
          "register_type": "input",
          "scale": 0.1,
          "max_temp": 30,
          "min_temp": 10,
          "scan_interval": 60,
          "precision": 1,
          "target_temp_register": 2,
          "target_temp_scale": 0.5,
          "hvac_mode_register": {
            "address": 10,
            "values": {
              "state_off": 0,
              "state_heat": 1,
              "state_cool": 2
            }
          }
        },
        {
          "name": "Heating_Zone_2",
          "address": 12,
          "register_type": "input",
          "scale": 0.1,
          "max_temp": 30,
          "min_temp": 10,
          "scan_interval": 60,
          "precision": 1,
          "target_temp_register": 7,
          "target_temp_scale": 0.5,
          "hvac_mode_register": {
            "address": 10,
            "values": {
              "state_off": 0,
              "state_heat": 1,
              "state_cool": 2
